function init(self)
	msg.post(".", "acquire_input_focus")

    if vibrate then
    	print("Can vibrate")
    else
    	print("Can't vibrate")
    end
end

function on_input(self, action_id, action)
	if action.released then
		if vibrate then
			vibrate.trigger()
		else
			print("Vibrate not available")
		end
	end
end